{
  "name": "cake-hash",
  "version": "0.0.1",
  "description": "CakePHP in Utility.Hash class like a collection manipulation. In JavaScript.",
  "main": "cake-hash.js",
  "scripts": {
    "start": "npm run watch",
    "test": "npm run lint && npm run mocha",
    "watch": "npm run watch:test & npm run watch:js",
    "watch:js": "watchify src/cake-hash.js -o cake-hash.js -t babelify -v -d",
    "watch:test": "mocha -w --compilers js:espower-babel/guess test/**/*.js",
    "build": "browserify src/cake-hash.js -o cake-hash.js -t babelify -p [ browserify-header --file src/header.js ] && npm run uglify",
    "uglify": "uglifyjs -c -o cake-hash.min.js cake-hash.js --comments",
    "mocha": "mocha --compilers js:espower-babel/guess test/**/*.js",
    "lint": "eslint src/**/*.js test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tsuyoshiwada/cake-hash.git"
  },
  "keywords": [
    "array",
    "object",
    "utility",
    "browser",
    "client",
    "server",
    "cakephp"
  ],
  "author": "tsuyoshiwada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/cake-hash/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/cake-hash",
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-header": "^0.9.2",
    "eslint": "^1.8.0",
    "espower-babel": "^4.0.0",
    "mocha": "^2.3.3",
    "power-assert": "^1.1.0",
    "uglify-js": "^2.6.0",
    "watchify": "^3.6.0"
  }
}
